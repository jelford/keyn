{
	"manifest_version": 2,
	"name": "keylinks",
	"version": "0.1",
	"description": "Navigate the world wide web with the power of your keyboard",
	"background": {
		"scripts": ["keylinks.js"]
	},
	"commands": {
		"toggle-feature": {
			"suggested_key": {
				"default": "Ctrl+Period"
			},
			"description": "Follow a link"
		}
	},
	"permissions": [
		"activeTab"
	]
}
